/**index.wxss**/


.top
{display: flex;
  width: 100%;
  height: 15%;

  background: #ff702a;
}
.logo
{
  
  width: 100px;
  height: 100px;
}
.logo image{ width: 60%;
  height: 60%;
  margin-top: 22px;
  margin-left: 14px;
    border-radius: 40px;
  }
.right
{vertical-align: top;
  height: 100%;
margin-top: 0;
width: auto;
flex-grow:1;

  
}
.title
{
  font-size: 16px;display: block;
  color: white;margin-top: 25px;
  margin-bottom: 5px;}
.location
{
  background: #fcb49e;
  display: inline-block;
  height: 20px;
  border:2px solid;
border-radius:25px;
}
.location text
{display: block;
  color: gray;
  font-size:13px;
  line-height: 20px;
  padding-left: 5px;
  padding-right: 5px;
}
page
{
background-color: #f8f8f8;

}
.brand
{
  
  margin-top: 10px;
  margin-left: 10px;

}
.brands
{
  display: inline-block;
  width: 100px;
  height: 120px;
}
.brand scroll-view
{
   white-space: nowrap;
}
.brands image
{
  width: 100px;
  height: 100px;
    border-radius: 50px;
}
.brand text
{display: block;
text-align: center;
}
.doblelist
{float: left;
width: 50%;


}
